{
  "name": "nimayneb/yawl",
  "description": "YAWL - Yet another wildcard library - Match pattern for strings with asterisk and query token",
  "keywords": [
    "wildcard",
    "pattern",
    "asterisk",
    "string",
    "strings",
    "query",
    "question mark",
    "questionmark",
    "token",
    "matcher",
    "match"
  ],
  "license": "GPL-3.0",
  "type": "library",
  "version": "2.0.0",
  "authors": [
    {
      "name": "Jan Runte",
      "email": "github-0801@nimayneb.de"
    }
  ],
  "require": {
    "php": "^7.4.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3.0",
    "rkr/wildcards": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "JayBeeR\\Wildcard\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JayBeeR\\Wildcard\\Tests\\": "Tests/"
    }
  }
}
